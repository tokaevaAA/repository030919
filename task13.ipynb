   "source": [
    "def fibonacci(n):\n",
    "    if (n==1 or n==2):\n",
    "        return 1\n",
    "    else:\n",
    "        return fibonacci(n-1) + fibonacci(n-2)\n",
    "    \n",
    "n=int(input(\"Enter the amount of the fibonacci numbers you want: \"))\n",
    "otv=fibonacci(n)\n",
    "print(\"The\",n,\"fibonacci is\",otv)"
   ]
